@import url('https://fonts.googleapis.com/css?family=Open+Sans:400,700')

html, body, .page
    width 100%
    height 100%
    margin 0

.test-list
    margin 15px

.vis-table
    height 300px

.vis-full
    width 100%
    height 100%

.containing-matrix
    width 100%

canvas
    display block

.overlay
    position fixed
    bottom 200px
    right 100px
    width 200px
    padding 20px
    background-color #ffeeaabb

.hidden
    display none
    visibility hidden

text:not(.fontawe)
    font-family Open Sans !important

#vis-element
    overflow hidden
