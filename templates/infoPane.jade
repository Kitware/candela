table.table.table-responsive.table-compact
  thead.result-labels
    th(colspan="2")
      .agg-label-header
        | Algorithm
    if algorithms.length >= 1 && aggTrends[algorithms[0]].length > 1
      th
        .agg-sparkline-header
          | Last 7 Days
    th(colspan="2")
      .agg-bullet-header
        | #{aggregate_metric_name}
  tbody
  each algorithm in algorithms
    tr.info-section-chart(id="#{algorithm}-aggregate-container")
      td.agg-status-dot
        svg.statusDot
            circle.none(id="#{algorithm}-aggregate-dot" r="10", cx="10", cy="10")
      td
        .agg-label
          | #{algorithm.toUpperCase()}
      if algorithms.length >= 1 && aggTrends[algorithms[0]].length > 1
        td
          .agg-sparkline(id="#{algorithm}-aggregate-sparkline")
            svg
      td
        .agg-value
          | #{Math.round(aggTrends[algorithm][aggTrends[algorithm].length-1] * 10000) / 10000}
      td
        .agg-bullet(id="#{algorithm}-aggregate-bullet")
          svg(id="#{algorithm}-aggregate-bullet-svg")
