table.table.table-striped.table-hover.table-bordered
  thead.result-labels
    td Dataset
    td ALGA
    td Sparse
    td BA
    td Track Length
  tbody
    each resultSet in resultsByDataset
      tr
        td(id="#{resultSet[0].dataset}-name") #{resultSet[0].dataset}
        each result in resultSet
          td.bullet-container(id="#{result.dataset}-#{result.metric}")
            svg
        td(id="#{resultSet[0].dataset}-length") #{resultSet[0].length}
