{
  "name": "candela",
  "version": "0.3.0",
  "description": "Visualization components for the web",
  "main": "dist/candela.js",
  "scripts": {
    "build": "webpack --config config/webpack.config.js",
    "build:reference": "cd app/resonant-reference-app && webpack",
    "dist": "webpack --config config/webpack-production.config.js && webpack --config config/webpack-production-minify.config.js",
    "test": "karma start config/karma-testing.conf.js | grep '^PhantomJS' | sed \"s/^.*LOG: '\\(.*\\)'$/\\1/\" | faucet",
    "test:raw": "karma start config/karma-testing.conf.js",
    "test:karma": "karma start config/karma-testing.conf.js --no-single-run",
    "cover": "karma start config/karma-coverage.conf.js",
    "cover:report": "http-server coverage -p ${PORT-3000}",
    "demo": "http-server build -p ${PORT-3000}"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:kitware/candela.git"
  },
  "author": "Roni Choudhury",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-istanbul-loader": "^0.1.0",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "backbone": "^1.2.3",
    "clean-webpack-plugin": "^0.1.7",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-promise": "^1.0.8",
    "eslint-plugin-standard": "^1.3.2",
    "expose-loader": "^0.7.1",
    "faucet": "0.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.9.0",
    "http-server": "^0.9.0",
    "jade": "^1.11.0",
    "jade-loader": "^0.8.0",
    "javascript-detect-element-resize": "^0.5.3",
    "json-loader": "^0.5.4",
    "karma": "^0.13.22",
    "karma-coverage": "^0.5.5",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-tap": "^1.0.4",
    "karma-tap-reporter": "0.0.6",
    "karma-webpack": "^1.7.0",
    "legacy-loader": "0.0.2",
    "less": "^2.3.1",
    "node-sass": "^3.4.2",
    "phantomjs-prebuilt": "^2.1.5",
    "raw-loader": "^0.5.1",
    "sass-loader": "^3.2.0",
    "script-loader": "^0.6.1",
    "semistandard-loader": "0.0.3",
    "strip-json-comments": "^2.0.1",
    "strip-json-comments-loader": "0.0.2",
    "style-loader": "^0.13.0",
    "stylus-loader": "^1.5.1",
    "tape": "^4.5.1",
    "telegraph-events": "^1.0.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.12"
  },
  "dependencies": {
    "brace": "^0.7.0",
    "d3": "^3.5.14",
    "datalib": "^1.6.3",
    "font-awesome": "^4.4.0",
    "font-awesome-webpack": "^0.0.4",
    "jquery": "^2.2.0",
    "LineUpJS": "git+https://github.com/Caleydo/lineup.js#51ae6f2bcb55aead3f63357864e90d5799789910",
    "underscore": "^1.8.3",
    "UpSet": "git+https://github.com/VCG/upset#6e8b7a172443dd6b4f4324d1a3368287e9e9c089",
    "vega": "^2.4.2"
  }
